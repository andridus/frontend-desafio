{
  "name": "frontend-desafio",
  "version": "1.0.0",
  "description": "Frontend de Teste para Agenda Edu",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "npm run -s test -- --watch",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -s -c 1",
    "build": "cross-env NODE_ENV=production webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andridus/frontend-desafio.git"
  },
  "keywords": [
    "frontend",
    "desafio",
    "agendaedu"
  ],
  "author": "Helder de Sousa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andridus/frontend-desafio/issues"
  },
  "homepage": "https://github.com/andridus/frontend-desafio#readme",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.7",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "offline-plugin": "^5.0.5",
    "postcss-loader": "^2.1.5",
    "preact-jsx-chai": "^2.2.1",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.11.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.0",
    "preact-render-to-string": "^3.7.0",
    "preact-router": "^2.6.1",
    "promise-polyfill": "^8.0.0",
    "proptypes": "^1.1.0",
    "serve": "^8.1.2",
    "workbox-webpack-plugin": "^3.2.0",
    "xhr": "^2.5.0"
  }
}
